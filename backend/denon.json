{
    "$schema": "https://deno.land/x/denon@2.3.3/schema.json",
    "watch": false,
    "watcher": {
        "interval": 300,
        "match": ["*.*", "src/**/*.*"],
        "skip": [],
        "legacy": false
    },
    "scripts": {
        "start": [
            {
                "cmd": "deno run main.ts",
                "desc": "run app server",
                "allow": [
                    "env",
                    "net",
                    "read"
                ],
                "watch": true,
                "unstable": true
            }
        ],
        "cache": "deno cache src/deps/*.ts"
    }
  }
